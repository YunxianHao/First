#include<stdio.h>
int prime(int n)
{
	int m, i;
	m = n;
	if (n < 2)
	{
		return -1;
	}
	else
	{
		for (i = 2; i < n; i++)
		{
			if (n % i == 0)
			{
				return 0;
				break;
			}
		}
		if (m == i)
		{
			return 1;
		}
	}
}
int main()
{
	int prime(int n);
	int n;
	scanf_s("%d", &n);
	printf("%d", prime(n));
	return 0;
}
